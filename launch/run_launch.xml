<launch>
  <arg name="device" default="/dev/ttyUSB0" />
  <arg name="rate" default="1000.0" />
  <arg name="sensor_frame_id" default="sensor" />
  <arg name="topic" default="force" />

  <node pkg="tmc_dynpick_driver" name="dynpick_driver_node" exec="dynpick_driver_node" >
    <param name="device" value="$(var device)" />
    <param name="rate" value="$(var rate)" />
    <param name="frame_id" value="$(var sensor_frame_id)" />
    <remap from="force" to="$(var topic)" />
  </node>
</launch>